{
    "version": 2,
    "builds": [
      { "src": "server.js", "use": "@now/node" }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/server.js",
        "methods": ["GET", "POST", "PUT", "PATCH", "DELETE"],
        "headers": {
          "Cache-Control": "max-age=3600"
        }
      },
      {
        "src": "/app/(.*)",
        "dest": "/server.js",
        "methods": ["GET", "HEAD", "POST", "PUT", "PATCH", "DELETE", "OPTIONS"]
      },
      {
        "src": "/pages/(.*)",
        "dest": "/pages/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/server.js"
      }
    ]
  }
  