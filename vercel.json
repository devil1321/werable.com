{
  "version": 2,
  "builds": [
    {
      "src": "next.config.js",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    { "src": "/api/.*", "dest": "/api" },
    { "src": "/.*", "dest": "/" }
  ],
  "functions": {
    "api/*": {
      "maxDuration": 60 
    },
    "api/printful": {
      "maxDuration": 60 
    }
  }
}