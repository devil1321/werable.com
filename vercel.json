{
    "version": 2,
    "builds": [
      { "src": "server.js", "use": "@now/node" }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "headers": {
          "x-vercel-timeout": "60" 
        },
        "dest": "/api/$1"
      },
      { "src": "/(.*)", "dest": "server.js" }
    ]
  }